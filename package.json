{
    "name": "social-graphics-library",
    "version": "1.1.0",
    "description": "A script for generating social media logos and banners",
    "scripts": {
        "build": "webpack",
        "run": "webpack serve",
        "test": "node tests",
        "asbuild:untouched": "asc src/glc.ts --target debug",
        "asbuild:optimized": "asc src/glc.ts --target release",
        "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized"
    },
    "main": "./dist/sgl.bundle.js",
    "module": "src/glc.ts",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/JosunLP/Social-graphics-library"
    },
    "keywords": [
        "fearnixx-grafik-generator",
        "social-graphics-library",
        "sgl",
        "fearnixx",
        "fng",
        "josunlp",
        "fngg"
    ],
    "author": "josunlp",
    "license": "MIT",
    "dependencies": {
        "@assemblyscript/loader": "^0.17.1"
    },
    "bugs": {
        "url": "https://github.com/JosunLP/Social-graphics-library/issues"
    },
    "homepage": "https://github.com/JosunLP/Social-graphics-library",
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/preset-env": "^7.11.5",
        "@types/node": "^14.10.2",
        "assemblyscript": "^0.17.1",
        "babel-loader": "^8.0.5",
        "ts-loader": "^8.0.4",
        "typescript": "^4.0.3",
        "webpack": "^4.44.2",
        "webpack-cli": "^4.1.0",
        "webpack-dev-server": "^3.11.0"
    }
}